<!-- Page Heading -->
<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">
      My Dashboard
    </h1>
  </div>
</div>
<!-- /.row -->

<div class="row control-group">
  <a href="/tickets/new" type="button" class="btn btn-success">Make a ticket</a>
</div>

<div class="row">
  <br>
  <div class="col-lg-12">
    <div class="panel panel-info">
      <div class="panel-heading">
        <h3 class="panel-title"> My all support tickets</h3>
      </div>
      <div class="panel-body">
        <table id="tickets" class="display" cellspacing="0" width="100%">
          <thead>
          <tr>
            <th>SL</th>
            <th>Ticket Title</th>
            <th>Ticket Description</th>
            <th>Status</th>
            <th>Agent Name</th>
            <th>Created Date</th>
            <th>Details</th>
          </tr>
          </thead>
          <tbody>
          <tr>
            <td>Tiger Nixon</td>
            <td>System Architect</td>
            <td>Edinburgh</td>
            <td>61</td>
            <td>2011/04/25</td>
            <td>$320,800</td>
            <td>$320,800</td>
          </tr>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
<!-- /.row -->

<% content_for :special_script do %>
    <script>
        $(document).ready(function() {
            $('#tickets').DataTable();
        } );
    </script>
<% end %>